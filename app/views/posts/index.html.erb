            <div class="projects-section ">
                <div class="projects-section-header ">
                    <p>Tweets</p>
                    <p class="time "><%= Date.today %></p>
                </div>
                <%= form_with model: @post, local: true do |f| %>
                    <%= render 'layouts/error_messages', model: @post %>
