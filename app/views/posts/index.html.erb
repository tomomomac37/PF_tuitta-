                      <%= f.text_area :body, size: "40x5", :placeholder => '„ÅÑ„Åæ„Å©„Çì„Å™Ê∞óÊåÅ„Å°Ôºü' %>
                      <div class="image_wrapper">
                        <div class="item_image_btn">
                          <%= f.attachment_field :image, id: "file" %>
                        </div>
                        <div id="img_field" onClick="$('#file').click()" >
                          <p class="image-select">ÁîªÂÉèÈÅ∏Êäû</p>
                        </div>
                          <p class="image-select"><%= f.submit "„Éà„Ç•„Ç§„Éº„Éà", class: "btn btn-primary" %></p>
                      </div>
                    </div>
